{
  "name": "hannahhall.github.io",
  "version": "1.0.0",
  "author": "Hannah Hall",
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "chokidar-cli": "^2.0.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "postcss-focus-within": "^3.0.0"
  },
  "scripts": {
    "build:style": "node-sass sass/style.scss style.css --output-style expanded && postcss -r style.css",
    "build": "run-p \"build:*\"",
    "watch": "chokidar \"**/*.scss\" -c \"npm run build\" --initial"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "bulma-extensions": "^6.2.7"
  }
}
